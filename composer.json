{
    "name": "atomescrochus/laravel-gracenote",
    "type": "library",
    "description": "A Laravel 5.* package to search the Gracenote Web API.",
    "keywords": [
        "atomescrochus",
        "laravel-gracenote",
        "gracenote"
    ],
    "homepage": "https://github.com/atomescrochus/laravel-gracenote",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Philippe Murray",
            "email": "jp@atomescroch.us",
            "homepage": "http://jpmurray.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/support": "~5.1",
        "nategood/httpful": "^0.2.20"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Atomescrochus\\Gracenote\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atomescrochus\\Gracenote\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "config": {
        "sort-packages": true
    }
}
